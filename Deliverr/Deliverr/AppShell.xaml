<?xml version="1.0" encoding="UTF-8" ?>
<Shell xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       xmlns:pages="clr-namespace:Deliverr"
       x:Class="Deliverr.AppShell">

    <FlyoutItem Title="Home" Route="WelkomPagina">
        <ShellContent ContentTemplate="{DataTemplate pages:WelkomPagina}" />
    </FlyoutItem>

    <FlyoutItem Title="Bestellingen" Route="BestellingPagina">
        <ShellContent ContentTemplate="{DataTemplate pages:BestellingPagina}" />
    </FlyoutItem>

    <FlyoutItem Title="Account" Route="AccountPagina">
        <ShellContent ContentTemplate="{DataTemplate pages:AccountPagina}" />
    </FlyoutItem>

</Shell>
